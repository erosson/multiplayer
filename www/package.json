{
  "name": "www",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "yarn serve",
    "docker": "docker buildx build -t ghcr.io/erosson/multiplayer/www:latest --target server ..",
    "docker:buildrun": "yarn docker && yarn docker:run",
    "docker:run": "docker run -p 8080:80 -it erosson/multiplayer/www",
    "docker:ghcr": "docker run -p 8080:80 ghcr.io/erosson/multiplayer/www:main"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shared": "workspace:shared"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@webpack-cli/generators": "^3.0.1",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "version": "1.0.0",
  "description": "My webpack project"
}
