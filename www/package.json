{
  "name": "www",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "yarn serve",
    "docker": "docker buildx build -t ghcr.io/erosson/multiplayer/www:latest --target www ..",
    "docker:buildrun": "yarn docker && yarn docker:run",
    "docker:run": "docker run -p 8080:80 -it ghcr.io/erosson/multiplayer/www:latest",
    "docker:ghcr": "docker run -p 8080:80 ghcr.io/erosson/multiplayer/www:latest"
  },
  "dependencies": {
    "d3-quadtree": "d3/d3-quadtree#find-filter",
    "fp-ts": "^2.13.1",
    "graphology": "^0.25.1",
